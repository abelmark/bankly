<h1>Users#show</h1>
<p>Find me in app/views/users/show.html.erb</p>

<%= cl_image_tag(@picture,
  :width => 400, :height => 400,
  :crop => :fill, :gravity => :face)
%>  

<%= bootstrap_form_tag url: '/profile', :method => :patch do |f| %>
  <%= f.file_field :picture %>
  <%= f.submit %>
<% end %>

